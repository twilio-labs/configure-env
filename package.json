    "dev": "tsc --watch",
    "test": "jest",
    "cm": "git-cz",
    "prepack": "run-s clean build",
    "release": "standard-version"
  },
  "keywords": [
    "dotenv",
    "setup",
    "configuration",
    "env",
    "environment"
  ],
  "author": "Dominik Kundel <dkundel@twilio.com> (https://twilio.com/labs)",
  "license": "MIT",
  "devDependencies": {
    "@types/common-tags": "^1.8.0",
    "@types/jest": "^24.0.23",
    "@types/mock-fs": "^4.10.0",
    "@types/prompts": "^2.0.3",
    "@types/yargs": "^13.0.3",
    "common-tags": "^1.8.0",
    "cz-conventional-changelog": "^3.0.2",
    "debug": "^4.1.1",
    "eslint": "^6.6.0",
    "eslint-plugin-prettier": "^3.1.1",
    "jest": "^24.9.0",
    "mock-fs": "^4.10.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0",
    "standard-version": "^7.0.1",
    "ts-jest": "^24.1.0",
    "typescript": "^3.7.2"
  },
  "dependencies": {
    "@types/node": "^14.0.13",
    "log-symbols": "^3.0.0",
    "prompts": "^2.3.0",
    "yargs": "^15.0.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "files": [
    "bin/",
    "dist/!(__tests__)",
    "README.md",
    "LICENSE"
