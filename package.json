{
  "name": "configure-env",
  "version": "1.0.0",
  "description": "Configures your project's environment by creating a .env file.",
  "main": "dist/index.js",
  "bin": {
    "configure-env": "./bin/configure-env.js"
  },
  "scripts": {
    "clean": "rimraf dist/",
